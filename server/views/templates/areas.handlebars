<div class="container mt-2">
  <table class="table table-dark">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Location</th>
        <th scope="col">Led By</th>
      </tr>
    </thead>
    <tbody>
      {{#each areas}}
      <tr>
        <td>{{name}}</td>
        <td>{{location}}</td>
        <td>{{ledby}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>